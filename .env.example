# Application (Client) ID from the General Information page
APP_ID=

# Discord Bot Token (Bot section in Developer Portal)
DISCORD_TOKEN=

# Public key from the General Information page (needed for signature verification in interactions)
PUBLIC_KEY=

# Optioneel: kanalen op naam blokkeren (kleine letters, komma-gescheiden)
DENY_CHANNEL_NAMES=algemeen,general,regels,rules,welkom,welcome

# Laat scheduler alleen kanalen updaten met bestaande pollberichten
ALLOW_FROM_PER_CHANNEL_ONLY=true

# Optional: Your bot's command prefix
# COMMAND_PREFIX=!

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
# LOG_LEVEL=INFO
